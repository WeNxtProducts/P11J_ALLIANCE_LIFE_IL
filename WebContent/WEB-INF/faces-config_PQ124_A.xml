<?xml version="1.0"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<managed-bean>
		<managed-bean-name>PQ124_A_COMPOSITE_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PQ124_A.PQ124_A_COMPOSITE_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>PQ124_A_SEARCH_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PQ124_A.PQ124_A_SEARCH_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>PQ124_A_PT_WAKALAH_DAILY_DRIP_1</from-outcome>
			<to-view-id>
				/jsf/PQ124_A/PQ124_A_PT_WAKALAH_DAILY_DRIP_1.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>PA_PROFIT</from-outcome>
			<to-view-id>
				/jsf/PQ124_A/PQ124_A_PS_IL_CONT_PAID_INVEST.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>


</faces-config>
