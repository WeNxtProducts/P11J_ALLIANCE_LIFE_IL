<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
	
	
    <managed-bean>
        <managed-bean-name>GNMNF004_A_APAC_COMPOSITE_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.GNMNF004_A_APAC.GNMNF004_A_APAC_COMPOSITE_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>GNMNF004_A_APAC_SEARCH_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.GNMNF004_A_APAC.GNMNF004_A_APAC_SEARCH_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    		<!-- Search Page to USER Group entry -->
	<navigation-rule>
		
		<navigation-case>
			<from-outcome>gotoUserGroupEntry</from-outcome>
			<to-view-id>
				/jsf/GNMNF004_A_APAC/GNMNF004_A_APAC_MENU_USER_GROUP.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!--  From User Group Entry to Search Page (Back Action) -->
		<navigation-rule>
		<from-view-id>
			/jsf/GNMNF004_A_APAC/GNMNF004_A_APAC_MENU_USER_GROUP.xhtml
		</from-view-id>
		<navigation-case>
			<from-outcome>searchPage</from-outcome>
			<to-view-id>
				/jsf/GNMNF004_A_APAC/GNMNF004_A_APAC_SEARCH.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
