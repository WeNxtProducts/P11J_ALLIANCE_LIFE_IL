	<navigation-rule>
		<navigation-case>
			<from-outcome>goToPymtDtls</from-outcome>
			<to-view-id>
				/jsf/PILT016A_APAC/PILT016A_APAC_PT_IL_PYMT_DTLS.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>


	<navigation-rule>
		<navigation-case>
			<from-outcome>goToPolicyDtls</from-outcome>
			<to-view-id>
				/jsf/PILT016A_APAC/PILT016A_APAC_POLICY_DET.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>




</faces-config>
