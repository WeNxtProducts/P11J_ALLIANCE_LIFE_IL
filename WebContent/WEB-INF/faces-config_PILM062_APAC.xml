<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
    <managed-bean>
        <managed-bean-name>PILM062_APAC_COMPOSITE_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.PILM062_APAC.PILM062_APAC_COMPOSITE_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PILM062_APAC_SEARCH_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.PILM062_APAC.PILM062_APAC_SEARCH_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <!-- Search Page to product group -->
    
	<navigation-rule>
		
		<navigation-case>
			<from-outcome>goToProdGroup</from-outcome>
			<to-view-id>/jsf/PILM062_APAC/PILM062_APAC_PM_IL_PROD_GROUP.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	 <!-- product group to Search Page -->
	<navigation-rule>
		
		<navigation-case>
			<from-outcome>backToSearch</from-outcome>
			<to-view-id>/jsf/PILM062_APAC/PILM062_APAC_SEARCH.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
    
</faces-config>
