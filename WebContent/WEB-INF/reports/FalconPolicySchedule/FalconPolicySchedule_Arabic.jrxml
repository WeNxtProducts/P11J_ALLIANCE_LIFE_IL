				<textElement textAlignment="Right">
					<font fontName="DejaVu Sans" pdfEncoding="Cp1256"/>
				</textElement>
				<text><![CDATA[بدون تغطية أي مخاطر إذا لم يدفع القسط خلال فترة السماح.]]></text>
			</staticText>
			<staticText>
				<reportElement x="52" y="543" width="467" height="20" uuid="52a2f821-19aa-461b-bed6-ed05978097ac"/>
