	<subDataset name="TOTAL_PREMIUM" uuid="dbb429f4-1a2b-4be3-988f-3e85c9c1294d">
		<parameter name="SUB_POL_SYS_ID" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT TOT_PREM * MOP TOT_PREMIUM
  FROM (SELECT (NVL(POL_LC_BASIC_PREM, 0) + NVL(POL_LC_ADDL_PREM, 0) +
               nvl((SELECT SUM(POC_LC_VALUE)
                      FROM PT_IL_POL_CHARGE
                     WHERE POC_POL_SYS_ID = $P{SUB_POL_SYS_ID}
                       AND POC_CHRG_CODE IN ('GOV.FEE', 'POL-FEE')),
                    0)) TOT_PREM,
               POL_MODE_OF_PYMT_RATE MOP
          FROM PT_IL_POLICY
         WHERE POL_SYS_ID = $P{SUB_POL_SYS_ID})]]>
		</queryString>
		<field name="TOT_PREMIUM" class="java.math.BigDecimal"/>
	</subDataset>
