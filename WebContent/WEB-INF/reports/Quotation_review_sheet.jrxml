	<subDataset name="Medical_YN" uuid="0307a1fa-0dd8-44b8-864a-58c1a970ed3c">
		<parameter name="POL_SYS_ID" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT PC_DESC FROM PM_CODES,PT_IL_POL_MED_EXAM_DTL
WHERE PC_TYPE  ='IL_MED_EXM'
AND PMED_EXAM_CODE = PC_CODE
AND PMED_POL_SYS_ID =$P{POL_SYS_ID}]]>
		</queryString>
		<field name="PC_DESC" class="java.lang.String"/>
	</subDataset>
	<parameter name="POL_SYS_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT POL_SYS_ID,POL_CLASS_CODE,
       (SELECT LIV_CODE_DESC
          FROM PP_LIST_ITEM_VALUES
         WHERE LIV_SYS_PARAM = 'TITLE'
           AND LIV_CODE = 1) TITLE,
       CUST_CODE,
       CUST_NAME,
       CUST_ADDR1,
       CUST_ADDR2,
       CUST_ADDR3,
       POL_PERIOD TERM,
       DECODE(POL_MODE_OF_PYMT,
              'Y',
              'YEARLY',
              'H',
              'HALF-YEARLY',
              'Q',
              'QUARTERLY',
              'M',
              'MONTHLY',
              'SINGLE') FREQUENCY,
       POL_NO,
       POL_ISSUE_DT RISK_DATE,
       POL_START_DT,
       POL_FC_FIRST_PREM, -- Newly Added
       (SELECT PS_VALUE FROM PP_SYSTEM WHERE PS_Code = 'BMI_VAL') BMI,
       (SELECT PROD_DESC FROM PM_IL_PRODUCT WHERE PROD_CODE = POL_PROD_CODE) PROD_DESC,
       POAD_ASSURED_NAME PROPOSER_NAME,
       POAD_DOB,
       POAD_AGE,
       POAD_HEIGHT,
       POAD_WEIGHT,
       DECODE(POAD_HEIGHT_UNIT, 'M', 'Meters', 'C', 'CentiMeters', 'Feet') H_UNIT,
       DECODE(POAD_WEIGHT_UNIT, 'K', 'KiloGrams', 'Pounds') W_UNIT,
       POAD_BMI,
       NVL((SELECT PPL_LC_MIN_PREM
             FROM PM_IL_PROD_PREM_LIMIT
            WHERE PPL_PROD_CODE = POL_PROD_CODE
              AND PPL_MODE_OF_PYMT = POL_MODE_OF_PYMT
              AND POL_PERIOD BETWEEN PPL_FM_TERM AND PPL_TO_TERM),
           0) "PPL_LC_MIN_PREM",
       (SELECT BANK_NAME FROM FM_BANK WHERE BANK_CODE = POL_BANK_CODE) BANK_NAME,
       POL_BANK_DIVN BRANCH_NAME,
       POL_FC_SUM_ASSURED BASIC_SA,
       POL_FC_BASIC_PREM BASIC_PREM,
       POL_OCC_RATE BASIC_RATE,
       (SELECT ROUND((POC_LC_VALUE * (DECODE(POC_APPLY_PERIOD,
                                             'P',
                                             POL_MODE_OF_PYMT_RATE,
                                             1))),
                     CURR_DECIMAL)
          FROM PT_IL_POL_CHARGE
         WHERE POC_POL_SYS_ID = POL_SYS_ID
           AND POC_CHRG_CODE LIKE '%POL%') POL_FEES,
       (SELECT ROUND((POC_LC_VALUE * (DECODE(POC_APPLY_PERIOD,
                                             'P',
                                             POL_MODE_OF_PYMT_RATE,
                                             1))),
                     CURR_DECIMAL)
          FROM PT_IL_POL_CHARGE
         WHERE POC_POL_SYS_ID = POL_SYS_ID
           AND POC_CHRG_CODE LIKE '%GOV%') GOV_FEES,
       (SELECT SUM(PDL_FC_DISC_LOAD_VALUE)
          FROM PT_IL_POL_DISC_LOAD
         WHERE PDL_POL_SYS_ID = POL_SYS_ID
           AND PDL_DISC_LOAD_TYPE = 'L') TOT_LOAD,
       (SELECT SUM(PDL_FC_DISC_LOAD_VALUE)
          FROM PT_IL_POL_DISC_LOAD
         WHERE PDL_POL_SYS_ID = POL_SYS_ID
           AND PDL_DISC_LOAD_TYPE = 'D') TOT_DISC,
       (SELECT POBH_BROKER_CODE
          FROM PT_IL_POL_BROKER_HEAD
         WHERE POBH_POL_SYS_ID = POL_SYS_ID
           AND POBH_SRNO = '01') AGENT_CODE,
		   (SELECT CUST_NAME FROM PM_CUSTOMER WHERE CUST_CODE = (SELECT POBH_BROKER_CODE
          FROM PT_IL_POL_BROKER_HEAD
         WHERE POBH_POL_SYS_ID = POL_SYS_ID
           AND POBH_SRNO = '01')) AGENT_NAME,
       (SELECT POBH_BROKER_CODE
          FROM PT_IL_POL_BROKER_HEAD
         WHERE POBH_POL_SYS_ID = POL_SYS_ID
           AND POBH_SRNO = '02') SO_CODE,
		   (SELECT CUST_NAME FROM PM_CUSTOMER WHERE CUST_CODE = (SELECT POBH_BROKER_CODE
          FROM PT_IL_POL_BROKER_HEAD
         WHERE POBH_POL_SYS_ID = POL_SYS_ID
           AND POBH_SRNO = '02')) SO_NAME,
       (SELECT POBH_BROKER_CODE
          FROM PT_IL_POL_BROKER_HEAD
         WHERE POBH_POL_SYS_ID = POL_SYS_ID
           AND POBH_SRNO = '03') SM_CODE,
		   (SELECT CUST_NAME FROM PM_CUSTOMER WHERE CUST_CODE = (SELECT POBH_BROKER_CODE
          FROM PT_IL_POL_BROKER_HEAD
         WHERE POBH_POL_SYS_ID = POL_SYS_ID
           AND POBH_SRNO = '03')) SM_NAME,
       POL_CR_UID,
       POL_UPD_UID,
       DECODE(POL_CLASS_CODE, '5', 'Munich-Re', 'Swiss-Re') Class_Type,
       (SELECT NVL(PDL_EMR_RATE, 0)
          FROM PT_IL_POL_DISC_LOAD
         WHERE PDL_POL_SYS_ID = $P{POL_SYS_ID}
           AND PDL_DISC_LOAD_CODE = 'DIS-COMM') COMM_REDUCED,
       (SELECT PDL_EMR_RATE
          FROM PT_IL_POL_DISC_LOAD
         WHERE PDL_POL_SYS_ID = $P{POL_SYS_ID}
           AND PDL_DISC_LOAD_CODE = 'FIC-LOAD') FIC_CHARGE,
       F_MEDI_EXAM_CODE($P{POL_SYS_ID}) MEDICAL_REPORT,
       NVL((SELECT (POAD_AGE + POAC_PERIOD)
             FROM PT_IL_POL_ADDL_COVER
            WHERE POAC_POL_SYS_ID = POL_SYS_ID
              AND POAC_POAD_SYS_ID = POAD_SYS_ID
              AND POAC_COVER_CODE = 'PPDA'),
           0) PPD_AGE,
       NVL((SELECT (POAD_AGE + POAC_PERIOD)
             FROM PT_IL_POL_ADDL_COVER
            WHERE POAC_POL_SYS_ID = POL_SYS_ID
              AND POAC_POAD_SYS_ID = POAD_SYS_ID
              AND POAC_COVER_CODE = 'PTDA'),
           0) PTD_AGE,
           NVL((SELECT SUM(PDL_FC_DISC_LOAD_VALUE)
          FROM PT_IL_POL_DISC_LOAD
         WHERE PDL_POL_SYS_ID = POL_SYS_ID
           AND PDL_DISC_LOAD_CODE = 'MPL'),0)  MIN_PREM_LOAD,
         NVL((SELECT POBD_RATE
          FROM PT_IL_POL_BROKER_DTL, PT_IL_POL_BROKER_HEAD
         WHERE POBD_POBH_SYS_ID = POBH_SYS_ID
           AND POBH_POL_SYS_ID = $P{POL_SYS_ID}
           AND POBH_SRNO = 1
          AND '1' BETWEEN POBD_YEAR_FROM AND POBD_YEAR_TO),0) COMM_RATE,--newly added
          NVL((SELECT F_COMM_VALUE($P{POL_SYS_ID}) FROM DUAL),0) COMM_VALUE,
      (select decode(poad_catg_code,'1','Male','2','Female','Male') Category
  from pt_il_pol_assured_dtls
 where poad_pol_sys_id = POL_SYS_ID )CATEGORY_FLD
  FROM PT_IL_POLICY, PT_IL_POL_ASSURED_DTLS, PM_CUSTOMER, PM_CURRENCY
 WHERE POAD_POL_SYS_ID = POL_SYS_ID
   AND POL_CUST_CODE = CUST_CODE
   AND POL_DS_TYPE = '1'
   AND POL_SYS_ID = $P{POL_SYS_ID}
   AND POL_CUST_CURR_CODE = CURR_CODE]]>
	</queryString>
