	<subDataset name="PolicyCover_Dataset" uuid="ed239053-8756-46bc-9e09-cfd8626cd9eb">
		<parameter name="POL_SYS_ID" class="java.lang.String"/>
		<parameter name="SUB_POL_SYS_ID" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT COVER_DESC, ROWNUM ANNEX_NO, POAC_LC_SA,POAC_RATE,POAC_PERIOD, NVL(POAC_LC_PREM,0), EMR_RATE
FROM (
SELECT COVER_DESC         COVER_DESC,
       POL_LC_SUM_ASSURED POAC_LC_SA,
       POL_BASIC_RATE     POAC_RATE,
       POL_PERIOD         POAC_PERIOD,
       (POL_LC_BASIC_PREM*POL_MODE_OF_PYMT_RATE)  POAC_LC_PREM,
       POL_EMR_RATE EMR_RATE
  FROM PT_IL_POLICY, PM_IL_COVER
 WHERE POL_SYS_ID = $P{POL_SYS_ID}
   AND COVER_CODE = POL_PLAN_CODE
UNION ALL
SELECT COVER_DESC   COVER_DESC,
       POAC_LC_SA   POAC_LC_SA,
       POAC_RATE    POAC_RATE,
       POAC_PERIOD  POAC_PERIOD,
       (POAC_LC_PREM*POL_MODE_OF_PYMT_RATE) POAC_LC_PREM,
       NVL(POAC_EMR_RATE,0) EMR_RATE
  FROM PT_IL_POL_ADDL_COVER, PT_IL_POLICY, PM_IL_COVER
 WHERE POL_SYS_ID = POAC_POL_SYS_ID
   AND POAC_POL_SYS_ID = $P{POL_SYS_ID}
   AND COVER_CODE = POAC_COVER_CODE
   )]]>
		</queryString>
		<field name="COVER_DESC" class="java.lang.String"/>
		<field name="ANNEX_NO" class="java.math.BigDecimal"/>
		<field name="POAC_LC_SA" class="java.math.BigDecimal"/>
		<field name="POAC_RATE" class="java.math.BigDecimal"/>
		<field name="POAC_PERIOD" class="java.math.BigDecimal"/>
		<field name="NVL(POAC_LC_PREM,0)" class="java.math.BigDecimal"/>
		<field name="EMR_RATE" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="RI" uuid="daa81668-64c7-4dd8-9943-7995a412bbab">
		<parameter name="SUB_POL_SYS_ID" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT RPA_COVER_CODE,
       COVER_DESC,
       RPA_RI_FC_SA,
       RD_TAR_RATE,
       RPA_RI_FC_PREM,
       POL_PERIOD
  FROM PT_IL_RI_DETL, PT_IL_RI_PREM_ALLOC, PT_IL_POLICY, PM_IL_COVER
 WHERE RD_POL_SYS_ID = POL_SYS_ID
   AND RPA_POL_SYS_ID = RD_POL_SYS_ID
   AND RPA_RD_SYS_ID = RD_SYS_ID
   AND RD_COVER_CODE = POL_PLAN_CODE
   AND COVER_CODE = RD_COVER_CODE
   AND RD_POL_SYS_ID = $P{SUB_POL_SYS_ID}
UNION ALL
SELECT POAC_COVER_CODE,
       COVER_DESC,
       RPA_RI_FC_SA,
       RD_TAR_RATE,
       RPA_RI_FC_PREM,
       POAC_PERIOD
  FROM PT_IL_RI_DETL, PT_IL_RI_PREM_ALLOC, PT_IL_POL_ADDL_COVER, PM_IL_COVER
 WHERE RD_POL_SYS_ID = POAC_POL_SYS_ID
   AND RPA_POL_SYS_ID = RD_POL_SYS_ID
   AND RPA_RD_SYS_ID = RD_SYS_ID
   AND RD_COVER_CODE = POAC_COVER_CODE
   AND COVER_CODE = POAC_COVER_CODE
   AND RD_POL_SYS_ID = $P{SUB_POL_SYS_ID}]]>
		</queryString>
		<field name="RPA_COVER_CODE" class="java.lang.String"/>
		<field name="COVER_DESC" class="java.lang.String"/>
		<field name="RPA_RI_FC_SA" class="java.math.BigDecimal"/>
		<field name="RD_TAR_RATE" class="java.math.BigDecimal"/>
		<field name="RPA_RI_FC_PREM" class="java.math.BigDecimal"/>
		<field name="POL_PERIOD" class="java.math.BigDecimal"/>
	</subDataset>
