	<parameter name="POL_SYS_ID_PARAM" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT  POL_PLAN_CODE           Q1_PLAN_CODE,
POL_NO                  Q1_POL_NO,
POL_SYS_ID              Q1_POL_SYS_ID,
PROD_DESC               Q1_PROD_NAME,
CUST_NAME               Q1_CUST_NAME,
CUST_ADDR1||', '||CUST_ADDR2 ||', '|| CUST_ADDR3 Q1_ADDR1,
CUST_AREA ||', '||   CUST_REGION ||', '||   CUST_CITY ||'   '||NVL2(CUST_POSTAL_CODE ,'POSTAL CODE -'||CUST_POSTAL_CODE, CUST_POSTAL_CODE) Q1_ADDR2,
CUST_COUNTRY ||'     '||NVL2(CUST_PHONE2,'Mobile No  -'||CUST_PHONE2,CUST_PHONE2)    Q1_CUST_COUNTRY ,
CUST_STATE             Q1_CUST_STATE,POL_PROD_CODE Q1_POL_PROD_CODE_DESC,'  ' Q1_NULL,
POL_ASSURED_NAME        Q1_ASSURED_NAME,
POL_FC_SUM_ASSURED      Q1_SA,
(POL_FC_SUM_ASSURED * (1.5 / 100)) Q1_COVER_PAYOUT,
POL_FC_BASIC_PREM      Q1_POL_FC_BASIC_PREM,
POAD_DOB Q1_DOB,
POAD_AGE                Q1_AGE,
POL_ISSUE_DT            Q1_COMMENCE_DT,
COVER_DESC                Q1_COVER_DESC,
to_date(POL_EXPIRY_DT,'DD/MM/RRRR') Q1_EXPIRY_DT,
POL_PERIOD              Q1_TERM,
pol_prem_pay_yrs Q1_PAYING_PERIOD,
DECODE(POL_MODE_OF_PYMT,'M','Monthly','Q','Quarterly','Y','Yearly','S','Single','H','Half Yearly') Q1_MOP,
DECODE(POAD_CATG_CODE,'1','Male','2','Female','3','Others') Q1_GENDER,
DECODE (POAD_AGE_PROOF_FLAG,'Y','Yes','N','No')             Q1_AGE_PROOF ,
'Product Code: '             Q1_PLA_CODE,
'Product Name: '             Q1_PLAN_NAME,
'Policy Number: '             Q1_POLICY_NUMBER,
'Policyholder: '             Q1_POLICY_HOLDER,
'(Name and Address of the Policyholder shall be printed here)'   Q1_NAME_ADDRESS,
'Life Insured:'     Q1_LIFE_INSURED,
'Gender:'          Q1_GEN,
'Date of Birth:'  Q1_BIRTH,
'Policy Installment Premium:'     Q1_INS_PREM,
'Policy Commencement Date:'   Q1_COM_DATE,
'Premium Frequency:'                 Q1_FREQUENCY,
'Currency of the Policy:'              Q1_CURRENCY,
'Age:'         Q1_AGE_TEXT,
'Age Admitted(Yes/No):'              Q1_AGE_ADMIT,
'Policy Term:'                               Q1_POLICY_TERM,
'Premium Paying Period:'              Q1_PERIOD,
'Policy Expiry Date:'                        Q1_EXP_DATE,
'Saudi Riyal'                       Q1_RUPEE,
pol_lc_first_prem               Q1_INST_PREM,
prod_code Q1_prod_code,
poad_age_admitted Q1_poad_age_admitted,
15785 POL_SYS_ID,
        pol_period coverage_term,POL_LAST_INST_DT Premium_Due,
       pol_lc_sum_assured Coverage_Sum_Assured,
pol_start_dt coverage_commnc_dt,
       POAD_AGE_ADMITTED Q1_POAD_AGE_ADMITTED
 FROM PT_IL_POLICY,PM_CUSTOMER, PT_IL_POL_ASSURED_DTLS,PM_IL_PRODUCT,PM_IL_COVER
 WHERE POL_CUST_CODE =CUST_CODE
 AND   POL_PROD_CODE =PROD_CODE
 AND    POL_PLAN_CODE = COVER_CODE
AND    COVER_CODE =PROD_PLAN_CODE
 AND   POL_SYS_ID    =POAD_POL_SYS_ID
 AND   POAD_SERIAL_NO =1
 AND   POL_SYS_ID    = $P{POL_SYS_ID_PARAM}]]>
	</queryString>
