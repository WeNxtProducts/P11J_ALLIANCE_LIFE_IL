<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
      
    <managed-bean>
        <managed-bean-name>CTRL_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.PILP022.CTRL_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>DUMMY_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.PILP022.DUMMY_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
   
    <managed-bean>
        <managed-bean-name>PILP022_PT_AGENT_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.PILP022.PT_AGENT_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
     <managed-bean>
        <managed-bean-name>PILP022_COMPOSITE_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.PILP022.PILP022_COMPOSITE_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PILP022_PS_AGENT_COMM_VALUES_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.PILP022.PS_AGENT_COMM_VALUES_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    
    </managed-bean>
    <managed-bean>
    	<managed-bean-name>loginManagedBean</managed-bean-name>
    	<managed-bean-class>
    		com.iii.pel.login.model.LoginManagedBean
    	</managed-bean-class>
    	<managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
    	<from-view-id>/jsf/xhtml/login.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>success</from-outcome>
    		<to-view-id>/jsf/xhtml/home.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>

</faces-config>
