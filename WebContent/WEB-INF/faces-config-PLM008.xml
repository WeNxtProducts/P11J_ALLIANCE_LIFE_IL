<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<!-- faces config entries for Codes Master PLM008 -->
	<managed-bean>
		<managed-bean-name>PLM008_PM_CODES_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PLM008.PM_CODES_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<converter>
		<description>checkbox Converter</description>
		<converter-id>customConverter</converter-id>
		<converter-class>
			com.iii.pel.forms.PLM008.CheckBoxConverter
		</converter-class>
	</converter>

	<managed-bean>
		<managed-bean-name>PLM008_SEARCH_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PLM008.PLM008_SEARCH_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>PLM008_Utility</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PLM008.ClassMasterUtility
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<navigation-case>
			<from-outcome>codeMaster</from-outcome>
			<to-view-id>/jsf/PLM008/PLM008_PM_CODES.xhtml</to-view-id>
		</navigation-case>
		<!-- <navigation-case>
			<from-outcome>PLM008_SEARCH_PAGE</from-outcome>
			<to-view-id>
				/jsf/PLM008/PLM008_PM_CODES_SEARCH.xhtml
			</to-view-id>
		</navigation-case> -->
	</navigation-rule>
	<!-- End of faces config entries for Codes Master PLM008 -->
</faces-config>
