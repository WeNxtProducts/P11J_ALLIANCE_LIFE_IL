<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
    <managed-bean>
        <managed-bean-name>PM290_PASSWORD_SETUP_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.PM290.PM290_PASSWORD_SETUP_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
    	<navigation-case>
  		  <from-outcome>PM290_PASSWORD_SETUP</from-outcome>
		  <to-view-id>/jsf/PM290/PM290_PASSWORD_SETUP.xhtml</to-view-id>
		</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<navigation-case>
  			<from-outcome>PM290_RESET_EXPIRED_PASSWORD</from-outcome>
			<to-view-id>/jsf/PM290/PM290_RESET_EXPIRED_PASSWORD.xhtml</to-view-id>
		</navigation-case>
    </navigation-rule>
    
    
       <navigation-rule>
    <navigation-case>
  			<from-outcome>GO_BACK</from-outcome>
			<to-view-id>/jsf/login/login.xhtml</to-view-id>
		</navigation-case>
    </navigation-rule>
    
    
    
    
</faces-config>
