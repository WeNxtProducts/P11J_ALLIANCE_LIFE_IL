<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
    
    <managed-bean>
		<managed-bean-name>GNMNF006_A_APAC_COMPOSITE_ACTION</managed-bean-name>
		<managed-bean-class>com.iii.pel.forms.GNMNF006_A_APAC.GNMNF006_A_APAC_COMPOSITE_ACTION</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>GNMNF006_A_APAC_SEARCH_ACTION</managed-bean-name>
