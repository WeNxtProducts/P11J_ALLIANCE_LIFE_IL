<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<managed-bean>
		<managed-bean-name>BP_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILM003_APAC.BP_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>BP_BL_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILM003_APAC.BP_BL_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>BUTTON_PALETTE_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILM003_APAC.BUTTON_PALETTE_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>COPY_PLAN_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILM003_APAC.COPY_PLAN_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>CTRL_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILM003_APAC.CTRL_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>DUMMY_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILM003_APAC.DUMMY_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ERROR_HELP_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILM003_APAC.ERROR_HELP_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>PILM003_APAC_PM_IL_PLAN</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILM003_APAC.PM_IL_PLAN
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
			PILM003_APAC_PM_IL_PLAN_COMPOSITE_ACTION
		</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILM003_APAC.PM_IL_PLAN_COMPOSITE_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>planMasterSearch</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILM003_APAC.PlanMasterSearch
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<description>
			On outcome of "goToPlanDetails" the flow should go to Main
			xhtml from Search Page
		</description>
		<from-view-id>
			/jsf/PILM003_APAC/PILM003_APAC_PM_IL_PLAN_SEARCH_PAGE.xhtml
		</from-view-id>
		<navigation-case>
			<from-outcome>goToPlanDetails</from-outcome>
			<to-view-id>
				/jsf/PILM003_APAC/PILM003_APAC_PM_IL_PLAN.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<description>
			On outcome of "goToSearchPage" the flow should go to Search
			Page
		</description>
		<navigation-case>
			<from-outcome>goToSearchPage</from-outcome>
			<to-view-id>
				/jsf/PILM003_APAC/PILM003_APAC_PM_IL_PLAN_SEARCH_PAGE.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>planMaster</from-outcome>
			<to-view-id>
				/jsf/PILM003_APAC/PILM003_APAC_PM_IL_PLAN.jsf
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>reductionSA</from-outcome>
			<to-view-id>
				/jsf/PILM003_APAC/PILM003_APAC_PM_IL_RED_SA.jsf
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>surrenderFactor</from-outcome>
			<to-view-id>
				/jsf/PILM003_APAC/PILM003_APAC_PM_IL_SURR_PAID_FACTOR.jsf
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>ageSetup</from-outcome>
			<to-view-id>
				/jsf/PILM003_APAC/PILM003_APAC_PM_IL_PLAN_AGE_SETUP.jsf
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>MOP</from-outcome>
			<to-view-id>
				/jsf/PILM003_APAC/PILM003_APAC_PM_IL_PLAN_MOP.jsf
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>copyAction</from-outcome>
			<to-view-id>
				/jsf/PILM003_APAC/PILM003_APAC_COPY_PLAN.jsf
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>gotoBonus</from-outcome>
			<to-view-id>
				/jsf/PILM037/PILM037_PM_IL_BONUS_SETUP.jsf
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>goToPlanMaster</from-outcome>
			<to-view-id>
				/jsf/PILM003_APAC/PILM003_APAC_PM_IL_PLAN.jsf
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
