<?xml version="1.0" encoding="UTF-8"?>

<faces-config>

    <managed-bean>
        <managed-bean-name>PP_LOV_ITEM_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.PP_LOV_ITEM.PP_LOV_ITEM_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
     <managed-bean>
        <managed-bean-name>PP_LOV_ITEM_SEARCH_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.pel.forms.PP_LOV_ITEM.PP_LOV_ITEM_SEARCH_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <navigation-rule>
		<from-view-id>/jsf/PP_LOV_ITEM/PP_LOV_ITEM_SEARCH.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>lovDetails</from-outcome>
			<to-view-id>
				/jsf/PP_LOV_ITEM/PP_LOV_ITEM.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/jsf/PP_LOV_ITEM/PP_LOV_ITEM.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>backToSearch</from-outcome>
			<to-view-id>
				/jsf/PP_LOV_ITEM/PP_LOV_ITEM_SEARCH.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	    <navigation-rule>
		<from-view-id>/jsf/PP_LOV_ITEM/PP_LOV_ITEM_SEARCH.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>NewRecord</from-outcome>
			<to-view-id>
				/jsf/PP_LOV_ITEM/PP_LOV_ITEM.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>
   
</faces-config>
