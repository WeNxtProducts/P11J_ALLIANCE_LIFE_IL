<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

	<!-- ManagedBean for PILP004 -->
	<managed-bean>
		<managed-bean-name>PILP004_BP_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILP004.BP_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
			PILP004_BUTTON_PALETTE_ACTION
		</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILP004.BUTTON_PALETTE_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>PILP004_CTRL_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILP004.CTRL_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>PILP004_DUMMY_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILP004.DUMMY_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>PILP004_ERROR_HELP_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILP004.ERROR_HELP_ACTION
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>PILP004_MATURITY_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILP004.MATURITY_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
			PILP004_PT_IL_PROCESS_ACTION
		</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PILP004.PT_IL_PROCESS_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<navigation-case>
			<from-outcome>openscreen</from-outcome>
			<to-view-id>
				/jsf/PILM038/PILM038_PM_IL_CLAIM_COVER.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>searchScreen</from-outcome>
			<to-view-id>/jsf/PILM038/CLAIM_COVER_SEARCH.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		 <from-view-id>/jsf/PILP004/PILP004_MATURITY.jsp</from-view-id> 
		<navigation-case>
			<from-outcome>true</from-outcome>
			<to-view-id>
				/jsf/PILP004/PILP004_PT_IL_PROCESS.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- End of faces config entries for plan master  PILP004 -->
	<navigation-rule>
		<from-view-id>/jsf/jsp/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/jsf/jsp/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/jsf/PILP001_APAC/Main.jsp</from-view-id>
		<navigation-case>
			<from-outcome>quotation</from-outcome>
			<to-view-id>
				/jsf/PILP001_APAC/PILP001_APAC_DUMMY.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
	 <navigation-case>
			<from-outcome>PILP004_CTRL</from-outcome>
			<to-view-id>/jsf/PILP004/PILP004_CTRL.xhtml</to-view-id>
		</navigation-case>
 </navigation-rule>
 <navigation-rule>
	 <navigation-case>
			<from-outcome>PILP004_MATURITY</from-outcome>
			<to-view-id>/jsf/PILP004/PILP004_MATURITY.xhtml</to-view-id>
		</navigation-case>
 </navigation-rule>
	<!--   
		previous
		<navigation-rule>
		<from-view-id>/jsf/PILT004/</from-view-id>
		
		</navigation-rule> -->
	<validator>
		<validator-id>ClassCodeValidator</validator-id>
		<validator-class>
			com.iii.pel.forms.PILM001.ClassCodeValidator
		</validator-class>
	</validator>
</faces-config>
