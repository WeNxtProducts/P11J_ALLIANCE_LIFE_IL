<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
    <managed-bean>
        <managed-bean-name>SSO_MANAGER_SEARCH_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.premia.common.commonimpl.sso.ssomanager.SSO_MANAGER_SEARCH_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
		<managed-bean-name>SSOAction</managed-bean-name>
		<managed-bean-class>
			com.iii.premia.common.commonimpl.sso.SSOAction
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
    <managed-bean>
        <managed-bean-name>SSO_MANAGER_COMPOSITE_ACTION</managed-bean-name>
        <managed-bean-class>com.iii.premia.common.commonimpl.sso.ssomanager.SSO_MANAGER_COMPOSITE_ACTION</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <navigation-case>
            <from-outcome>SSO_MANAGER_SEARCH</from-outcome>
            <to-view-id>/jsf/SSO/SSO_MANAGER_SEARCH_PAGE.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>PM_LOGIN_SWITCH</from-outcome>
            <to-view-id>/jsf/SSO/PM_LOGIN_SWITCH.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>PM_APPLICATION_DTLS</from-outcome>
            <to-view-id>/jsf/SSO/PM_APPLICATION_DTLS.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
