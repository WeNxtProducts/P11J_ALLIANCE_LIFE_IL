<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<managed-bean>
		<managed-bean-name>PM076_A_COMPOSITE_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PM076_A.PM076_A_COMPOSITE_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>PM076_A_PM_BROKER_COMM_HDR_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PM076_A.PM_BROKER_COMM_HDR_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>PM076_A_SEARCH_ACTION</managed-bean-name>
		<managed-bean-class>
			com.iii.pel.forms.PM076_A.PM076_A_SEARCH_ACTION
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<navigation-case>
			<from-outcome>PM076_A_HEADER</from-outcome>
			<to-view-id>
				/jsf/PM076_A/PM076_A_PM_BROKER_COMM_HDR.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>PM076_A_DETAIL</from-outcome>
			<to-view-id>
				/jsf/PM076_A/PM076_A_PM_BROKER_COMM_DTL.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>PM076_A_MAIN_HEADER</from-outcome>
			<to-view-id>
				/jsf/PM076_A/PM076_A_PM_BROKER_COMM_HDR.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/jsf/PM076_A/PM076_A_PM_BROKER_COMM_HDR.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>searchpage</from-outcome>
			<to-view-id>/jsf/PM076_A/PM076_A_SEARCH.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>	
