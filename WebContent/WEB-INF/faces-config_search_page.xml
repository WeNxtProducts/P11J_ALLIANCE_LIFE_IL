<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
     <managed-bean>
        <managed-bean-name>searchActionBean</managed-bean-name>
        <managed-bean-class>com.iii.premia.common.commonimpl.searchpageimpl.SearchAction</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
     	<navigation-case>
